<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="stellarnear.aquene_dealer.Activities.MainActivityFragmentKi">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="gestion du ki"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearKiFrag"
        android:layout_width="wrap_content"
        android:orientation="vertical"
        android:layout_height="wrap_content"></LinearLayout>


    <ImageButton
        tools:ignore="ContentDescription"
        android:id="@+id/button_frag_ki_to_main"
        android:layout_width="@dimen/size_icon_main_actions"
        android:layout_height="@dimen/size_icon_main_actions"
        android:layout_margin="@dimen/fab_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ki_back"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:backgroundTint="@android:color/transparent"/>

</android.support.constraint.ConstraintLayout>
